<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pucmm.proyectofinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoFinal">
        <activity
            android:name=".roomviewmodel.activities.CategoryRoomActivity"
            android:exported="true" />
        <activity
            android:name=".roomviewmodel.activities.CategoryMainActivity"
            android:exported="true" />
        <activity
            android:name=".roomviewmodel.activities.ProductMainActivity"
            android:exported="true" />
        <activity
            android:name=".roomviewmodel.activities.ProductRoomActivity"
            android:exported="true" />
        <activity
            android:name=".roomviewmodel.activities.MainRoomActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".roomviewmodel.activities.EditRoomActivity"
            android:exported="true" />
    </application>

</manifest>